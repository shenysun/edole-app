        "line-column-path": "2.0.0",
        "open-editor": "3",
        "pinia": "^2.0.11",
        "quasar": "^2.6.0",
        "simple-git": "^3.17.0",
        "vue": "^3.0.0",
        "vue-router": "^4.0.0"
    },
    "devDependencies": {
        "@quasar/app-vite": "^1.0.0",
        "@types/node": "^12.20.21",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "autoprefixer": "^10.4.2",
        "compressing": "^1.9.0",
